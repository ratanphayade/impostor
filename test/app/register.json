 {
    "method": "POST",
    "endpoint": "/register/{id}",
    "evaluators": [
      {
        "response": {
          "label": "success",
          "body": "{\"status\": \"registered\", \"id\":\"{{ copy body id.id }}\"}",
          "latency": 0,
          "status_code": 200,
          "headers": {
            "Content-Type": "application/json"
          }
        },
        "rules": []
      },
      {
        "response": {
          "label": "failure",
          "body": "{\"status\": \"duplicate\"}",
          "latency": 0,
          "status_code": 201,
          "headers": {
            "Content-Type": "application/json"
          }
        },
        "rules": [
          {
            "target": "body",
            "modifier": "id",
            "value": "true"
          }
        ]
      }
    ]
  }
